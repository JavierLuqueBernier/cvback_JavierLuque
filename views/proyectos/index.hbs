<h2 class="mt-5">Lista de proyectos</h2>
<div class="proyectos row">
    {{#each proys}}
    <div class="col-3">
        <div class="proyecto ">
            <h4>{{this.titulo}}</h4>
            <p>{{this.descripcion}}</p>
            <p>
                <a href="URL">{{this.url}}</a>
            </p>
            <p>{{this.cliente}}</p>
            <p>{{this.categoria}}</p>
        </div>
    </div>
    {{/each}}
</div>